[user]
	name = Gavin Engel
	email = gavin@engel.com
[push]
	default = simple
[url "https://"]
	insteadOf = git://
[alias]
	a = add
	ci = commit -v
	cm = commit -m
	co = checkout
	f = fetch
	m = merge
	p = pull
	ps = push
	s = status
	st = status
	unadd = reset
	uncommit = reset --soft HEAD^
[credential]
	helper = !aws codecommit credential-helper $@
	UseHttpPath = true
